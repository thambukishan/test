{
  "name": "test",
  "description": "A simple test application",
  "homepage": "https://phovea.caleydo.org",
  "version": "1.0.0",
  "author": {
    "name": "The Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phovea/test/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thambukishan/test.git"
  },
  "files": [
    "src",
    "index.js",
    "phovea.js",
    "phovea_registry.js",
    "build",
    "requirements_dev.txt",
    "docker_packages.txt"
  ],
  "scripts": {
    "start": "webpack-dev-server --inline",
    "check": "flake8 --exclude=.git,venv,deploy,docs,__pycache__,node_modules",
    "pretest": "npm run compile",
    "test": "test ! -d tests || karma start",
    "prebuild": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test",
    "build": "webpack --env prod",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/test.tar.gz *",
    "predocker": "npm run build",
    "docker": "docker build -t test -f deploy/Dockerfile .",
    "docker:dev": "docker build -t phovea_server -f deploy/Dockerfile_dev .",
    "docs": "typedoc --options typedoc.json src/**.ts",
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/**.ts tests/**.ts",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack",
    "start:hot": "webpack-dev-server --inline --hot",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "private": true,
  "engines": {
    "npm": ">= 3",
    "node": ">= 6"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "awesome-typescript-loader": "3.0.3",
    "cache-loader": "1.2.0",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "extract-loader": "0.1.0",
    "ifdef-loader": "2.0.0",
    "file-loader": "0.10.0",
    "fork-ts-checker-webpack-plugin": "0.2.9",
    "html-loader": "0.4.4",
    "imports-loader": "0.7.0",
    "jasmine": "2.5.3",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "2.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.0",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "5.0.0",
    "style-loader": "0.13.1",
    "thread-loader": "1.1.2",
    "ts-loader": "3.1.1",
    "tslib": "1.5.0",
    "tslint": "4.4.2",
    "typedoc": "0.7.2",
    "typescript": "2.2.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "phovea_core": "github:phovea/phovea_core#develop",
    "phovea_d3": "github:phovea/phovea_d3#develop",
    "phovea_vis": "github:phovea/phovea_vis#develop",
    "phovea_ui": "github:phovea/phovea_ui#develop",
    "phovea_importer": "github:phovea/phovea_importer#develop",
    "phovea_vis_lineup": "github:phovea/phovea_vis_lineup#develop",
    "phovea_security_flask": "github:phovea/phovea_security_flask#v0.0.5",
    "phovea_clue": "github:phovea/phovea_clue#v0.0.5",
    "phovea_processing_queue": "github:phovea/phovea_processing_queue#v0.0.5",
    "tdp_core": "github:datavisyn/tdp_core#develop",
    "pathfinder_ccle": "github:Caleydo/pathfinder_ccle#master",
    "pathfinder_graph": "github:Caleydo/pathfinder_graph#master",
    "@types/d3": "3.5.36",
    "d3": "3.5.17",
    "@types/jquery": "2.0.33",
    "jquery": "3.1.1",
    "@types/papaparse": "4.1.28",
    "papaparse": "4.1.2",
    "@types/jqueryui": "1.11.31",
    "jqueryui": "1.11.1",
    "lineupjs": "github:Caleydo/lineupjs#master",
    "font-awesome": "4.6.3",
    "@types/bootstrap": "3.3.32",
    "bootstrap-sass": "3.3.7",
    "@types/select2": "4.0.30",
    "select2": "4.0.3",
    "moment": "2.18.1",
    "@types/jquery.scrollto": "1.4.27",
    "jquery.scrollto": "2.1.2",
    "lru-fast": "github:sgratzl/js-lru",
    "selectivity": "3.0.3",
    "@types/lodash": "4.14.38",
    "lodash": "4.16.6",
    "@types/marked": "0.0.28",
    "marked": "0.3.6",
    "@types/file-saver": "0.0.0",
    "file-saver": "1.3.3",
    "datavisyn-scatterplot": "github:datavisyn/datavisyn-scatterplot",
    "@types/colorbrewer": "1.0.29",
    "colorbrewer": "github:sgratzl/colorbrewer"
  }
}
